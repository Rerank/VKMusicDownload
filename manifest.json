{
 "manifest_version":2,
 "name":"VK Music Downloader",
 "version":"1.0",
 "description":"Downloader of mp3 files from vk.com",
 "permissions": [
  "https://vk.com/*",
  "tabs"
 ],
    "content_scripts": [ {
      "js":["jquery.js", "script.js"],
      "matches": [ "http://vk.com/*", "http://*.vk.com/*", "https://vk.com/*", "https://*.vk.com/*" ],
      "run_at": "document_end"
   } ],  
     "browser_action": {
     "default_title":"Downloader Extension"
     }
}